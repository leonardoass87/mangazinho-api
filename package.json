{
  "name": "mangazinho-api",
  "version": "1.0.0",
  "description": "API do Mangazinho — plataforma de leitura online de mangás com painel administrativo",
  "main": "src/server.js",
  "scripts": {
    "local": "cross-env NODE_ENV=local nodemon -e js,json,env src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon -e js,json,env src/server.js",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "build": "echo \"No build step required for Node.js\""
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "path": "^0.12.7",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.10"
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "js,json,env",
    "ignore": ["storage/*"],
    "delay": "150"
  },
  "engines": {
    "node": ">=18.17"
  },
  "keywords": [],
